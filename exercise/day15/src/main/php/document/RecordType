<?php

class RecordType {
    const INDIVIDUAL_PROSPECT = "IndividualPersonProspect";
    const LEGAL_PROSPECT = "LegalEntityProspect";
    const ALL = "All";

    private $value;

    public function __construct($value) {
        $this->value = $value;
    }

    public function getValue() {
        return $this->value;
    }
}

?>
